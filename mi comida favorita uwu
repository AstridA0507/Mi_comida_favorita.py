comidas = ["Pizza", "Hamburguesa", "Lasagna", "Baleadas", "Tamales", "Spaghetti", "Tacos", "Bagre en Salsa", "Bragre Frito", "Papas Fritas", "Arepas", "Arroz con Vegetales", "Pollo Frito", "Pavo Asado", Pato Asado", "Sopas", "Camarones", "Langostas", "Pescado Frito", "Empanadas"]

def imprimir_menu():
	print("Escoge tus 5 comidas favoritas")
	for comida in comidas: 
			print(comida)
			
			
imprimir_menu

